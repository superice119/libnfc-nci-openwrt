# NFC Configuration for OpenWrt
# Place custom NFC settings here

config nfc 'settings'
	option enabled '1'
	option debug_level '2'
	# I2C device (usually /dev/i2c-0 or /dev/i2c-1)
	option i2c_device '/dev/i2c-1'
	# GPIO pins for PN7160 control
	option gpio_irq '23'
	option gpio_ven '24'
	option gpio_dwl '22'
